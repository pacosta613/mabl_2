<h1>Leagues</h1>
<form action="/leagues" method="post">
  <ul>
  <% @leagues.each do |league| %>
    <label>
    <li><a href="/leagues/<%= league.id %>"><%= league.name %></a>
    </label>
  
    <label>
    <a href="/leagues/<%= league.id %>/edit">Edit</a></li></label>
    <br>
  <% end %>
  </ul>
</form>

<h1>Create a new league:</h1>
<form>
  <a href="/leagues/new">Add League</a>
</form>